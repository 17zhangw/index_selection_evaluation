{
    "database_system": "postgres",
    "benchmark_name": "tpch",
    "scale_factor": 10,
    "algorithms": [
        {
            "name": "microsoft",
            "parameters": {
                "max_indexes": [10, 40, 100],
                "max_indexes_naive": 2,
                "max_index_columns": 3
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "dexter",
            "parameters": {
                "min_saving_percentage": [0, 50, 80]
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "epic",
            "parameters": {
                "budget": [100, 2000, 5000]
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "ibm",
            "parameters": {
                "max_index_columns": 3,
                "budget": [100, 2000, 5000],
                "try_variation_seconds_limit": 1
            },
            "number_of_runs": 5,
            "timeout": 300
        },
        {
            "name": "drop_heuristic",
            "parameters": {
                "max_indexes": [10, 40, 100]
            },
            "number_of_runs": 5,
            "timeout": 300
        }
    ]
}
