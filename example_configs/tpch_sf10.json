{
    "database_system": "postgres",
    "benchmark_name": "tpch",
    "scale_factor": 10,
    "algorithms": [
      {
            "name": "microsoft",
            "parameters": {
                "max_indexes": [0, 1, 2, 4, 8, 16],
                "max_indexes_naive": 2,
                "max_index_width": 3
            },
            "number_of_actual_runs": 5,
            "timeout": 300
        },
        {
            "name": "dexter",
            "parameters": {
                "min_saving_percentage": [0, 10, 25, 50, 90, 100]
            },
            "number_of_actual_runs": 5,
            "timeout": 300
        },
        {
            "name": "epic",
            "parameters": {
                "budget_MB": [400, 800, 1600, 3200, 6400, 12800]
            },
            "number_of_actual_runs": 5,
            "timeout": 300
        },
        {
            "name": "ibm",
            "parameters": {
                "max_index_width": 3,
                "budget_MB": [400, 800, 1600, 3200, 6400, 12800],
                "try_variations_seconds": 1
            },
            "number_of_actual_runs": 5,
            "timeout": 300
        },
        {
            "name": "drop_heuristic",
            "parameters": {
                "max_indexes": [1, 2, 4, 8, 16]
            },
            "number_of_actual_runs": 5,
            "timeout": 300
        }
    ]
}
